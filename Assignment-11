// find factorial of n
import java.util.Scanner;
class Assignment11{
    public static void main(String[] args) {
        Scanner input=new Scanner(System.in);
        
        System.out.println("Enter value of n:");
        int n=input.nextInt();
        int result=1;
        for(int i=1;i<=n;i++){
            result=result*i;
        }
        System.out.print(result);
    }
}
