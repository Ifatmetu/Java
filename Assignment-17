// Create a pattern like following one if n=4
/*
 1
 1 0
 1 0 1
 1 0 1 0
 */
import java.util.Scanner;
public class Assignment17 {
   public static void main(String[] args) {
    Scanner input = new Scanner(System.in);
    int c=1;
    for(int i=1;i<=4;i++){
        for(int j=1;j<=i;j++){
            System.out.print(c);
            if(c==1){
                c=0;
            }else{
                c=1;
            }
        }
        c=1;
        System.out.println();
    }
  }
}
